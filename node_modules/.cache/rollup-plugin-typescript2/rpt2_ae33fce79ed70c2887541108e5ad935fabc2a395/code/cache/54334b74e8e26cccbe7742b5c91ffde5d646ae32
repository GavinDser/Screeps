{"code":"export const RemoteSourceHarvester = {\r\n    run: function (creep) {\r\n        if (creep.memory.working && creep.store.getUsedCapacity() == 0) {\r\n            creep.memory.working = false;\r\n        }\r\n        if (!creep.memory.working && creep.store.getFreeCapacity() == 0) {\r\n            creep.memory.working = true;\r\n        }\r\n        if (creep.memory.working) {\r\n            if (creep.memory.targetRoom == creep.room.name) {\r\n                if (creep.harvest(Game.getObjectById(creep.memory.sourceId)) == ERR_NOT_IN_RANGE) {\r\n                    creep.moveTo(Game.getObjectById(creep.memory.sourceId));\r\n                }\r\n            }\r\n            else {\r\n                creep.moveTo(new RoomPosition(17, 23, creep.memory.homeRoom));\r\n            }\r\n        }\r\n        else {\r\n            if (creep.room.name == creep.memory.homeRoom) {\r\n                for (const resourceType in creep.store) {\r\n                    if (creep.transfer(creep.room.storage, resourceType) == ERR_NOT_IN_RANGE) {\r\n                        creep.moveTo(creep.room.storage);\r\n                    }\r\n                }\r\n            }\r\n            else {\r\n            }\r\n        }\r\n    }\r\n};\r\n//# sourceMappingURL=RemoteSourceHarvester.js.map","references":[],"map":"{\"version\":3,\"file\":\"RemoteSourceHarvester.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/modules/Creep/RemoteSourceHarvester.ts\"],\"names\":[],\"mappings\":\"AAAA,MAAM,CAAC,MAAM,qBAAqB,GAAI;IAClC,GAAG,EAAE,UAAS,KAAW;QACrB,IAAG,KAAK,CAAC,MAAM,CAAC,OAAO,IAAI,KAAK,CAAC,KAAK,CAAC,eAAe,EAAE,IAAI,CAAC,EAAE;YAC3D,KAAK,CAAC,MAAM,CAAC,OAAO,GAAG,KAAK,CAAC;SAChC;QACD,IAAG,CAAC,KAAK,CAAC,MAAM,CAAC,OAAO,IAAI,KAAK,CAAC,KAAK,CAAC,eAAe,EAAE,IAAI,CAAC,EAAE;YAC5D,KAAK,CAAC,MAAM,CAAC,OAAO,GAAG,IAAI,CAAC;SAC/B;QACD,IAAI,KAAK,CAAC,MAAM,CAAC,OAAO,EAAC;YACrB,IAAI,KAAK,CAAC,MAAM,CAAC,UAAU,IAAI,KAAK,CAAC,IAAI,CAAC,IAAI,EAAC;gBAC3C,IAAG,KAAK,CAAC,OAAO,CAAC,IAAI,CAAC,aAAa,CAAC,KA