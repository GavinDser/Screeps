   }\r\n            else {\r\n                if (creep.memory.stateSwitch) {\r\n                    if (creep.harvest(creep.pos.findClosestByRange(FIND_SOURCES_ACTIVE)) == ERR_NOT_IN_RANGE) {\r\n                        creep.moveTo(creep.pos.findClosestByRange(FIND_SOURCES_ACTIVE), { visualizePathStyle: { stroke: '#ffaa00' } });\r\n                    }\r\n                    creep.upgradeController(creep.room.controller);\r\n                    creep.memory.stateSwitch = false;\r\n                }\r\n                else {\r\n                    creep.upgradeController(creep.room.controller);\r\n                    creep.memory.stateSwitch = true;\r\n                }\r\n            }\r\n        }\r\n    }\r\n};\r\n//# sourceMappingURL=Upgrader.js.map","references":[],"map":"{\"version\":3,\"file\":\"Upgrader.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/modules/Creep/Upgrader.ts\"],\"names\":[],\"mappings\":\"AAAA;;;;;;;GAOG;AAGH,MAAM,CAAC,MAAM,QAAQ,GAAG;IAEpB,4BAA4B;IAC5B,GAAG,EAAE,UAAS,KAAY;QAEtB,IAAG,KAAK,CAAC,MAAM,CAAC,OAAO,IAAI,KAAK,CAAC,KAAK,CAAC,eAAe,CAAC,IAAI,CAAC,EAAE;YAC1D,KAAK,CAAC,MAAM,CAAC,OAAO,GAAG,KAAK,CAAC;SACnC;QACD,IAAG,CAAC,KAAK,CAAC,MAAM,CAAC,OAAO,IAAI,KAAK,CAAC,KAAK,CAAC,eAAe,EAAE,IAAI,CAAC,EAAE;YAC5D,KAAK,CAAC,MAAM,CAAC,OAAO,GAAG,IAAI,CAAC;SAC/B;QACD,IAAG,KAAK,CAAC,MAAM,CAAC,OAAO,EAAE;YAClB,IAAG,KAAK,CAAC,iBAAiB,CAAC,KAAK,CAAC,IAAI,CAAC,UAAU,CAAC,IAAI,gBAAgB,EAAE;gBACnE,KAAK,CAAC,MAAM,CAAC,KAAK,CAAC,IAAI,CAAC,UAAU,EAAE,EAAC,kBAAkB,EAAE,EAAC,MAAM,EAAE,SAAS,EAAC,EAAC,CAAC,CAAC;aAClF;YACD,0FAA0F;YAC1F,0CAA0C;YAC1C,IAAI;SACP;aACI;YACD,IAAI,CAAC,KAAK,CAAC,MAAM,CAAC,gBAAgB,EAAC;gBAC/B,IAAI,OAAO,GAAG,KAAK,CAAC,IAAI,CAAC,IAAI,CAAC,eAAe,EAAE;oBAC3C,MAAM,EAAE,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,aAAa,IAAI,mBAAmB,IAAI,CAAC,CAAC,KAAK,CAAC,eAAe,CAAC,GAAG,IAAI;iBAC3F,CAAC,CAAA;gBACF,IAAI,MAAM,GAAG,CAAC,CAAC,MAAM,CAAC,OAAO,EAAE,CAAC,CAAqB,EAAC,EAAE,CAAC,CAAC,CAAC,KAAK,CAAC,eAAe,CAAC,CAAC,CAAC,OAAO,EAAE,CAAA;gBAC5F,IAAI,MAAM,CAAC,MAAM,EAAC;oB